/* Laser transmitter module test project
 * tutorial and circuit url http://osoyoo.com/?p=640
 */
char val;
void setup() {
    Serial.begin(9600); 
    pinMode(13, OUTPUT);  
}
void loop() {
    val=Serial.read();
    if(val=='1') {
        digitalWrite(13, HIGH);  
    }
    if(val=='0') {
        digitalWrite(13, LOW);  
    }
}
